# Google Earth Engine Configuration
GEE_SERVICE_ACCOUNT_KEY=path/to/your/service-account-key.json
GEE_PROJECT_ID=your-gee-project-id
GEE_USE_SERVICE_ACCOUNT=true

# Optional: For increased API quotas
GEE_HIGH_VOLUME_ENDPOINT=true

# Data Storage Configuration
LOCAL_DATA_PATH=./data
MINIMAL_STORAGE=true
MAX_STORAGE_DAYS=30

# Model Configuration
MODEL_RETRAIN_FREQUENCY_DAYS=7
ALERT_THRESHOLD_CONFIDENCE=0.8
PRESCRIPTION_MAP_RESOLUTION_M=30